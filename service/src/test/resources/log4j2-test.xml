<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <CustomLevels>
    <CustomLevel name="HOMEWORK" intLevel="350" />
  </CustomLevels>
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="HOMEWORK">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>